{
  "name": "face-crop-forge",
  "version": "1.6.1",
  "description": "Face detection and cropping tool using MediaPipe",
  "type": "module",
  "scripts": {
    "dev": "bun --hot src/server.ts",
    "start": "bun src/server.ts",
    "build": "bun build src/server.ts --outdir ./dist --target bun --minify --sourcemap",
    "build:types": "tsc --emitDeclarationOnly",
    "build:prod": "bun run build && bun run build:types",
    "serve:prod": "bun dist/server.js",
    "clean": "rimraf dist",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "rimraf": "^6.0.1",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "face-detection",
    "mediapipe",
    "image-processing",
    "crop"
  ],
  "author": "https://github.com/gregorycarnegie/",
  "license": "AGPL-3.0"
}
